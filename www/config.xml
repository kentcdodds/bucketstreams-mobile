<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bucketstreams.mobile" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bucket Streams</name>
    <description>
        A mobile app for Bucket Streams. See what you want, share what you want.
    </description>
    <author email="info@bucketstreams.com" href="http://bucketstreams.com/">
        Bucket Streams Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
